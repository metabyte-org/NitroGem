  "scripts": {
    "install:fix": "npm install --ignore-scripts && npm rebuild sqlite3",
    "start": "npm run check-env && npm run app:pre | react-scripts --openssl-legacy-provider start --kill-others --kill-others-on-fail",
    "prepare": "npm run app:pre | react-scripts --openssl-legacy-provider build --kill-others --kill-others-on-fail",
    "eject": "react-scripts eject",
    "app:pre": "node app/index.js",
    "check-env": "node ./check-environment.js",
    "test": "react-scripts test",
    "compile": "npx hardhat compile",
    "test:contracts": "npx hardhat test",
    "deploy:local": "npx hardhat run scripts/deploy.js --network hardhat",
    "deploy:sepolia": "npx hardhat run scripts/deploy.js --network sepolia",
    "deploy:mainnet": "npx hardhat run scripts/deploy.js --network mainnet",
    "sync-abi": "node scripts/sync-abi.js",
    "build:contracts": "npx hardhat compile && node scripts/sync-abi.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=18.19.0",
    "npm": ">=8.0.0",
    "yarn": ">=1.22.0"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "@openzeppelin/contracts": "^5.4.0",
    "chai": "^4.3.0",
    "ethereum-waffle": "^4.0.10",
    "hardhat": "^2.22.0"
  }
}
